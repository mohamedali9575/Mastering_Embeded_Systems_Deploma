#include<stdio.h>


void main(){
	char word[200];
	int n;
	printf("Enter the string : ");
	fflush(stdin); fflush(stdout);
	gets(word);
	n=strlen(word)-1;
	printf("Reverse string is: ");
	for(int i = n ;i>=0;i--){
		printf("%c",word[i]);
	}
}
