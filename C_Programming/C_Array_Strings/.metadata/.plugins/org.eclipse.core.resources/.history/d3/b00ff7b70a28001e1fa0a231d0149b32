#include<stdio.h>


void main(){
	char word[200];
	printf("Enter the string: ");
	fflush(stdin); fflush(stdout);
	gets(word);
	strlen(word);
	printf("Reverse string is: ");
	for(int i = strlen(word) ;i>=0;i--){
		printf("%c",word[i]);
	}
}
